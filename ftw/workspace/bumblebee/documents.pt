<ul class="ViewChooser">
    <li class="active"><span class="documentsGallery" i18n:translate="documents_pill_documents">Gallery</span></li>
    <li><a id="listdocuments" class="documentsList" href="#" i18n:translate="documents_pill_list">List</a></li>
</ul>
<div tal:define="previews view/previews" class="previewContainer">
<div tal:condition="previews">
<tal:loop repeat="preview previews">

<div class="imageContainer">
  <a class="colorboxLink previewitem" rel="entry"
     tal:attributes="href string:${preview/overlay_url};
                     id preview/uid">
                 <img tal:attributes="src preview/mimetype_image_url;
                                 title preview/mimetype_title;
                                 data-title preview/mimetype_title"
                 width="16"
                 class="file-mimetype" />

            <img tal:attributes="src preview/preview_image_url;
                                 alt preview/title"
                 width="200"
                 class="file-preview" />

  <b class="previewTitle" tal:content="preview/title" />
  <span tal:condition="preview/description" class="previewDesc" tal:content="preview/description" />
  </a>
</div>
</tal:loop>
</div>
<div tal:condition="not: previews">
    <p class="no_content" i18n:translate="label_no_contents">No Contents</p>
</div>
</div>